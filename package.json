{
  "name": "nmo",
  "version": "1.2.0",
  "description": "A tool to manage your CouchDB cluster",
  "main": "lib/nmo.js",
  "bin": "bin/nmo-cli.js",
  "scripts": {
    "transpile": "babel src -d lib",
    "prepublish": "make && npm run transpile",
    "test": "npm run lint && ./node_modules/.bin/mocha --compilers js:babel-register ./test/*.js",
    "lint": "eslint ./src ./test ./bin"
  },
  "author": "Robert Kowalski <rok@kowalski.gd>",
  "contributors": [
    "Garren Smith <garren@apache.org>"
  ],
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/robertkowalski/nmo/issues"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "src": "./src",
    "doc": "./doc",
    "man": "./man",
    "test": "./test",
    "website": "./website"
  },
  "dependencies": {
    "JSONStream": "^1.0.6",
    "babel-preset-es2015": "^6.1.18",
    "bluebird": "~3.1.1",
    "bulkbadger": "^1.0.0",
    "config-chain": "~1.1.8",
    "couchbulkimporter": "^1.0.0",
    "csv-parse": "^1.0.0",
    "ini": "~1.3.3",
    "mongodb": "^2.0.49",
    "nopt": "~3.0.1",
    "npmlog": "~2.0.0",
    "osenv": "~0.1.0",
    "prettyjson": "^1.1.3",
    "valid-url": "~1.0.9",
    "wreck": "~7.0.0",
    "xtend": "~4.0.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.5",
    "babel-polyfill": "^6.2.0",
    "babel-register": "~6.3.13",
    "eslint": "^1.9.0",
    "less": "~2.5.0",
    "marked-man": "~0.1.4",
    "mocha": "^2.3.4",
    "nock": "^4.0.0",
    "node-doc-generator": "robertkowalski/node-documentation-generator#sloppy"
  }
}
